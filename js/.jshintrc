// Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
// Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
// Documentation: http://www.jshint.com/docs/

{
  "browser": true,
  "globals": {
    "THREE": true,
    "$": true,
    "Handlebars": true,
    "Detector": true,
    "Spinner": true,
    "requirejs": true,
    "requestAnimationFrame": true
  },
  "esnext": true,
  "globalstrict": false,
  "quotmark": true,
  "smarttabs": true,
  "trailing": true,
  "undef": true,
  "unused": true,
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "immed": true,
  "latedef": true,
  "noarg": true,
  "noempty": true
}